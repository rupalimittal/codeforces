#include<iostream>
using namespace std;
int main()
{
    int n,t,i,flag=0;
        cin>>n>>t;
        int a[n];
    if(t==10)
    {
        if(n==1)
        cout<<"-1";
        else
        {


        for(i=0;i<n-1;i++)
        a[i]=1;
        a[i]=0;
        flag=1;
        }
        if(flag==1)
        {


        for(i=0;i<n;i++)
        cout<<a[i];
        }
    }
    else
    {


    for(i=0;i<n;i++)
    {
        a[i]=t;
    }
    for(i=0;i<n;i++)
    {
       cout<<a[i];
    }
    }

    return 0;
}
