#include<iostream>
using namespace std;
int main()
{
    int n,i,c=0;
    long long int x;
    int a[1000];
    char s[1000];

    cin>>n>>x;
    for(i=0;i<n;i++)
    {

    cin>>s[i];
    cout<<" ";
    cin>>a[i];
    }

        for(i=0;i<n;i++)
        {
          if(s[i]=='+')
          {
              x=x+a[i];

           }
           else
           if(x<a[i])
           {
               c++;
           }

             else
             x=x-a[i];

    }



cout<<x<<" "<<c;

return 0;
}
